{
  "name": "velog-web",
  "version": "3.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18.16"
  },
  "author": {
    "name": "velopert",
    "email": "public.velopert@gmail.com"
  },
  "keywords": [
    "velog"
  ],
  "scripts": {
    "dev": "pnpm env:copy -e development && next dev -p 3001 --turbo",
    "stage": "pnpm env:copy -e stage && pnpm start",
    "prod": "pnpm env:copy -e production && pnpm start",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 3001",
    "lint": "next lint",
    "env:copy": "tsx ./scripts/copyEnv.ts",
    "create-component": "tsx ./scripts/createComponent.ts",
    "create-svg-component": "tsx ./scripts/createSvgComponent.ts",
    "codegen": "dotenv -e .env.development graphql-codegen --config codegen.ts -r dotenv/config",
    "ssm": "tsx ./scripts/ssm/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.431.0",
    "@sentry/browser": "^7.75.0",
    "@tanstack/react-query": "^4.32.0",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.12.17",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "inquirer": "^9.2.7",
    "next": "^13.5.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-toastify": "^9.1.3",
    "react-use": "^17.4.0",
    "sangte": "^0.1.26",
    "sharp": "^0.32.4",
    "tsx": "^3.12.7",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@svgr/cli": "^8.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/gtag.js": "^0.0.12",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^5.59.11",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "^13.4.19",
    "husky": "^8.0.0",
    "husky-init": "^8.0.0",
    "prettier": "^2.8.8",
    "typescript": "5.1.3",
    "typescript-plugin-css-modules": "^5.0.1"
  }
}
