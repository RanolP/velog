    "reflect-metadata": "^0.1.13",
    "tsx": "^3.12.7",
    "tsyringe": "^4.7.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
